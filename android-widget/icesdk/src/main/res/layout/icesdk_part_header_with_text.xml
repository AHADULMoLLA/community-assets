<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:theme="@style/Icesdk.Theme"
    tools:background="@color/icesdk_blue"
    >

    <include
        android:id="@+id/icesdk_close_btn"
        layout="@layout/icesdk_close_btn"
        />

    <icesdk.IceTextView
        android:id="@+id/icesdk_textview31"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="64dp"
        android:layout_marginTop="64dp"
        android:gravity="center"
        android:text="@string/icesdk_you_are_invited_nto_ice_ice"
        android:textAppearance="@style/Icesdk.H2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

</androidx.constraintlayout.widget.ConstraintLayout>
