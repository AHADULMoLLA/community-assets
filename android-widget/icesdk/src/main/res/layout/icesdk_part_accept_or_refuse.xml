        android:text="@string/icesdk_do_you_feel_like_a_slave"
        android:textAppearance="@style/Icesdk.Body1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/icesdk_textview8"
        />

    <TextView
        android:id="@+id/icesdk_textview10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="@string/icesdk_take_action"
        android:textAppearance="@style/Icesdk.H5.Extrabold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/icesdk_textview9"
        />

